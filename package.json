{
  "name": "ant_hydrotable",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "antd": "^4.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
